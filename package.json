{
  "name": "tv-insight-hbbtv-tracking-script",
  "version": "2.3.3",
  "type": "module",
  "repository": "git@github.com:rbredtech/tv-insight-hbbtv-tracking-script.git",
  "author": "Andreas Jantschnig <andreas.jantschnig@tv-insight.com>",
  "license": "MIT",
  "engines": {
    "node": ">=24"
  },
  "scripts": {
    "mock-hbbtv-app": "nodemon --watch \"mock-hbbtv-application/**\" --ext \"ts,html\" --exec \"node --import=./register.js mock-hbbtv-application/index.ts\"",
    "mock-session-server": "nodemon --watch \"mock-session-application/**\" --ext \"ts\" --exec \"node --import=./register.js mock-session-application/index.ts\"",
    "mock": "concurrently \"yarn mock-session-server\" \"yarn mock-hbbtv-app\"",
    "dev": "concurrently \"yarn mock\" \"wait-on http://localhost:3000/health && wait-on http://localhost:8080/health && open http://localhost:8080\"",
    "test": "jest",
    "ci": "start-server-and-test 'yarn mock-session-server' http://localhost:3000/health 'yarn test'",
    "lint": "eslint src mock-hbbtv-application mock-session-application",
    "minify": "gulp"
  },
  "devDependencies": {
    "@babel/core": "^7.24.9",
    "@babel/preset-env": "^7.24.8",
    "@babel/preset-typescript": "^7.24.7",
    "@jest/types": "^30.2.0",
    "@tsconfig/node20": "^20.1.4",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.5",
    "@types/gulp": "^4.0.18",
    "@types/gulp-htmlmin": "^1.3.36",
    "@types/gulp-terser": "^1.2.6",
    "@types/jest": "^30.0.0",
    "@types/node": "^24",
    "@typescript-eslint/eslint-plugin": "^8.54.0",
    "@typescript-eslint/parser": "^8.54.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "babel-plugin-source-map-support": "^2.2.0",
    "concurrently": "^9.2.1",
    "cors": "^2.8.6",
    "dotenv-load": "^3.0.0",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-html": "^8.1.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.2.1",
    "express": "^5.1.0",
    "globals": "^17.1.0",
    "gulp": "^5.0.1",
    "gulp-htmlmin": "^5.0.1",
    "gulp-minify-inline": "^1.1.0",
    "gulp-terser": "^2.1.0",
    "jest": "^30.2.0",
    "jest-puppeteer": "^11.0.0",
    "nodemon": "^3.1.4",
    "prettier": "^3.7.1",
    "puppeteer": "^24.31.0",
    "source-map-support": "^0.5.21",
    "start-server-and-test": "^2.0.4",
    "ts-jest": "^29.2.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.54.0",
    "uuid": "^13.0.0",
    "wait-on": "^9.0.3",
    "winston": "^3.13.1",
    "yargs": "^18.0.0"
  }
}
